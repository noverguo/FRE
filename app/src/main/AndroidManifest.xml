<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nv.fre"
    android:versionCode="2"
    android:versionName="1.1" >

    <uses-sdk android:minSdkVersion="16"/>

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.DISABLE_KEYGUARD" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:name="com.nv.fre.FREApplication"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <meta-data
            android:name="xposedmodule"
            android:value="true" />
        <meta-data
            android:name="xposeddescription"
            android:value="@string/fuck_red_envelope" />
        <meta-data
            android:name="xposedminversion"
            android:value="54" />

        <activity
            android:name="com.nv.fre.ui.SettingActivity"
            android:screenOrientation="portrait"
            android:launchMode="singleInstance"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity
            android:name="com.nv.fre.ui.TransparentActivity"
            android:screenOrientation="portrait"
            android:launchMode="singleInstance"
            android:theme="@style/TransparentTheme">
        </activity>

        <receiver
            android:name="com.nv.fre.receiver.SettingReceiver"
            android:exported="true" >
            <intent-filter>
                <action android:name="com.nv.fuckredenvelope.receiver.SettingReceiver" />
            </intent-filter>
        </receiver>
        <receiver
            android:name="com.nv.fre.receiver.UnlockReceiver"
            android:exported="true" >
            <intent-filter>
                <action android:name="com.nv.fuckredenvelope.receiver.UnlockReceiver" />
            </intent-filter>
        </receiver>
        <receiver android:name=".receiver.CompleteReceiver">
            <intent-filter>
                <action android:name="android.intent.action.DOWNLOAD_COMPLETE"/>
            </intent-filter>
        </receiver>
    </application>

</manifest>